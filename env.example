# 环境变量配置示例
# 复制此文件为 .env 并根据需要修改配置

# 模型配置
PIPELINE_PATH=models/pipeline_new.pkl
SCHEMA_PATH=models/schema.json

# Web请求配置
REQUEST_TIMEOUT=12
MAX_RETRIES=3
REQUEST_DELAY_MIN=0.6
REQUEST_DELAY_MAX=1.0

# 缓存配置
CACHE_TTL_HOURS=24
ENABLE_CACHE=true

# 日志配置
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# OpenAI配置
OPENAI_MODEL=gpt-4o-mini
OPENAI_API_KEY=your_openai_api_key_here

# 性能配置
MAX_PREDICTION_TIME=1.0
ENABLE_PERFORMANCE_MONITORING=true

# 自定义用户代理（可选，用逗号分隔）
# CUSTOM_USER_AGENTS=Mozilla/5.0 (Custom Agent), Another Custom Agent
